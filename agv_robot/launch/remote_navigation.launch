<launch>
  <!-- Launch this file on remote computer for remote control and visualization -->
  <!-- joy control -->
  <node pkg = "agv_robot" type="joy_driver" name="joy_driver" output="screen"/>
  <!-- rviz -->
  <node pkg="rviz" type="rviz" name="rviz" required="true" args="-d $(find agv_robot)/rviz/navigation.rviz"/>
  <!-- rqt_reconfigure -->
  <node name="rqt_reconfigure" pkg="rqt_reconfigure" type="rqt_reconfigure" />
</launch>
